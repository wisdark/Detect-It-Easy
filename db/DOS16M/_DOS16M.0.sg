// Detect It Easy: detection rule file

init("operation system", "DOS16M");

function detect() {
    if (DOS16M.isVerbose()) {
        sName = DOS16M.getOperationSystemName();
        sVersion = DOS16M.getOperationSystemVersion();
        sOptions = DOS16M.getOperationSystemOptions();

        bDetected = true;
    }

    return result();
}
