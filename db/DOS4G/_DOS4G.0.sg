// Detect It Easy: detection rule file

init("operation system", "DOS4G");

function detect() {
    if (DOS4G.isVerbose()) {
        sName = DOS4G.getOperationSystemName();
        sVersion = DOS4G.getOperationSystemVersion();
        sOptions = DOS4G.getOperationSystemOptions();

        bDetected = true;
    }

    return result();
}
