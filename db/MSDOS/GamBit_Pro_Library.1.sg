init("installer", "GamBit Pro Library");

function detect() {
    if (MSDOS.compareEP("ba....2e8916....b430cd218b2e....8b1e....8edaa3....8c06") &&
        MSDOS.compareOverlay("'RS'000102")) {
            sOptions = "1994 by Nikita Ltd.";
            bDetected = true;
    }
    
    return result();
}