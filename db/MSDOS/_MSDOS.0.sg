// Detect It Easy: detection rule file

init("operation system", "MS-DOS");

function detect() {
    if (MSDOS.isVerbose()) {
        sName = MSDOS.getOperationSystemName();
        sVersion = MSDOS.getOperationSystemVersion();
        sOptions = MSDOS.getOperationSystemOptions();

        bDetected = true;
    }

    return result();
}