// Detect It Easy: detection rule file

init("protector", "Shrink");

function detect() {
    if (MSDOS.compareEP("509CFCBE....BF....57B9....F3A48B......BE....BF....F3A4C3")) {
        sVersion = "1.0";
        bDetected = true;
    } else if (MSDOS.compareEP("E9....509CFCBE....8BFE8CC805....8EC00657B9")) {
        sVersion = "2.0";
        bDetected = true;
    }

    return result();
}