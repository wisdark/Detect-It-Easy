init("compiler", "djgpp");

function detect() {
    if (MSDOS.compare("'go32stub'", 512)) {
        sVersion = "2";
        bDetected = true;
    } else if (MSDOS.isSignaturePresent(0, Math.min(8192, MSDOS.getSize()), "'StubInfoMagic!!'")) {
        sVersion = "1";
        bDetected = true;
    }

    return result();
}