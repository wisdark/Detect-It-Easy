// Detect It Easy: detection rule file

init("source", "Pascal");

function detect() {
    var sText = Binary.getHeaderString();
    /* if(/^unit/im.test(sText))
    {
        bDetected=1;
    } */
    if (/^\s*program\s.*\s*uses\s/im.test(sText)) {
        bDetected = true;
    }


    _setLang("Pascal", bDetected);

    return result();
}