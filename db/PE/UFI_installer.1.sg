// Detect It Easy: detection rule file
// Author: A.S.L. <asl@onet.eu> 2020.09.30
// Exeinfo Pe - don't detect this installer

init("installer", "UFI installer");

function detect() {
    if ((PE.compareEP("558BEC")) && (PE.resource["BINFILE"]) && (PE.resource["BINFILE_2"])) {
        sVersion = "1.X, Flash";
        bDetected = true;
    }

    return result();
}