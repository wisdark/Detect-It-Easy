init("installer", "Eschalon Installer");

function detect() {
    if (PE.compareEP("558bec81c4........53565733c08985........8985........e8$$$$$$$$e8$$$$$$$$31d28d")) {
        if (PE.compareOverlay("'EPSF'")) {
            bDetected = true;
            sVersion = PE.getFileVersion().replace(/\.0\.0$/, "");
        }
    }

    return result();
}