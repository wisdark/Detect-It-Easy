init("sfx", "12Ghosts Zip2");

function detect() {
    if (PE.compareEP("E8$$$$$$$$558BEC83EC..A1........8365....8365....5357BF........3BC7BB........74")) {
        if (PE.findSignature(PE.getOverlayOffset(), 0x1000, "'BZh91'") !== -1) {
            sOptions = "BZIP2";
            sVersion = "8.0";
            bDetected = true;
        }
    }

    return result();
}