// Detect It Easy: detection rule file

init("protector", "Goliath");

function detect() {
    if (PE.isNET()) {
        if (PE.isNetObjectPresent("ObfuscatedByGoliath")) {
            bDetected = true;
        } else if (PE.isSignatureInSectionPresent(0, "'.Goliath.NET.CodeShield.'")) {
            bDetected = true;
        }
    }

    return result();
}