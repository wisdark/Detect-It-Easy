// Detect It Easy: detection rule file

init("protector", "Cameyo");

function detect() {
    if (PE.isOverlayPresent() && PE.compareOverlay("'MZ'") && PE.getVersionStringInfo("ProductName") == "Cameyo Application Virtualization") {
        sVersion = PE.getFileVersion();
        bDetected = true;
    }

    return result();
}