// Detect It Easy: detection rule file

init("protector", ".NET Spider");

function detect() {
    if (PE.isNET()) {
        if (PE.isSignatureInSectionPresent(0, "'Protected_By_Attribute'00'NETSpider.Attribute'")) {
            sVersion = "0.5-1.3";
            bDetected = true;
        }
    }

    return result();
}