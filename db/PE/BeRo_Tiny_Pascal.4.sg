// Detect It Easy: detection rule file

init("compiler", "BeRo Tiny Pascal");

function detect() {
    if (PE.compareEP("E9........' Compiled by: BeRoTinyPascal - (C) Copyright 2006, Benjamin '27'BeRo'27' Rosseaux '")) {
        bDetected = true;
    }

    _setLang("Pascal", bDetected);

    return result();
}
