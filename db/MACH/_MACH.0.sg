// Detect It Easy: detection rule file

init("operation system", "macOS");

function detect() {
    if (MACH.isVerbose()) {
        sName = MACH.getOperationSystemName();
        sVersion = MACH.getOperationSystemVersion();
        sOptions = MACH.getOperationSystemOptions();

        bDetected = true;
    }

    return result();
}