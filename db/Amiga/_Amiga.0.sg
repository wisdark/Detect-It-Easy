// Detect It Easy: detection rule file

init("operation system", "Amiga");

function detect() {
    if (Amiga.isVerbose()) {
        sName = Amiga.getOperationSystemName();
        sVersion = Amiga.getOperationSystemVersion();
        sOptions = Amiga.getOperationSystemOptions();

        bDetected = true;
    }

    return result();
}